<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
  <display-name>CRUDOperation</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>com.demo.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/validation</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>registration</servlet-name>
  <servlet-class>com.demo.servlet.registrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>registration</servlet-name>
  <url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>show</servlet-name>
  <servlet-class>com.demo.servlet.showProduct</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>show</servlet-name>
  <url-pattern>/showprod</url-pattern>
  </servlet-mapping>

  <servlet>
  <servlet-name>add</servlet-name>
  <servlet-class>com.demo.servlet.AddProduct</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>add</servlet-name>
  <url-pattern>/addProduct</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
  <servlet-name>edit</servlet-name>
  <servlet-class>com.demo.servlet.EditProduct</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>edit</servlet-name>
  <url-pattern>/updateProd</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>update</servlet-name>
  <servlet-class>com.demo.servlet.upadateProduct</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>update</servlet-name>
  <url-pattern>/updateProd</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-name>delete</servlet-name>
  <servlet-class>com.demo.servlet.deleteProduct</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>delete</servlet-name>
  <url-pattern>/deleteProd</url-pattern>
  </servlet-mapping>
  
</web-app>